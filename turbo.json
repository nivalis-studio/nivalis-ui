{
	"$schema": "https://turborepo.com/schema.json",
	"globalPassThroughEnv": ["COREPACK_HOME", "ENABLE_EXPERIMENTAL_COREPACK"],
	"tasks": {
		"build": {
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"dependsOn": ["^build"],
			"env": ["NEXT_PUBLIC_VERCEL_URL", "NODE_ENV"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"]
		}
	}
}
